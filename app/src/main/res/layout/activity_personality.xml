<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="personalityVM" type="com.programmingstodo.app.modules.personality.data.viewmodel.PersonalityVM"/>
    </data>
    <LinearLayout android:id="@+id/linearThirteen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearAppBar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_border_name_black_900_radius_7"
                    android:id="@+id/btnArrowLeft"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrow_left" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPersonality"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{personalityVM.personalityModel.txtPersonality}"
                    style="@style/txtPoppinssemibold18" tools:text="@string/lbl_personality"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearPersonality"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearEditProfilePic"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_86pxh" android:paddingStart="@dimen/_86pxh">
                    <ImageView android:id="@+id/imageProfileImage"
                        android:layout_height="@dimen/_139pxh"
                        android:layout_width="@dimen/_139pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_profile_image}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_profile_image"/>
                    <TextView android:alpha="0.8"
                        android:ellipsize="end"
                        android:id="@+id/txtEditPhoto"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{personalityVM.personalityModel.txtEditPhoto}"
                        style="@style/txtPoppinsmedium12_1" tools:text="@string/lbl_edit_photo"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearUserName"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtUserName"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{personalityVM.personalityModel.txtUserName}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_username"/>
                    <EditText
                        android:background="@drawable/rectangle_border_name_onprimary_opacity_0_1_radius_10"
                        android:ellipsize="end"
                        android:fontFamily="@font/poppins"
                        android:hint="@string/lbl_adalahalcana"
                        android:id="@+id/etUserName"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@={personalityVM.personalityModel.etUserNameValue}"
                        android:textColor="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textColorHint="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:ignore="Autofill"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearFirstName"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFirstName"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{personalityVM.personalityModel.txtFirstName}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_first_name2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_name_onprimary_opacity_0_1_radius_10"
                        android:ellipsize="end"
                        android:fontFamily="@font/poppins"
                        android:hint="@string/lbl_alcanasatre"
                        android:id="@+id/etFirstName"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@={personalityVM.personalityModel.etFirstNameValue}"
                        android:textColor="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textColorHint="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:ignore="Autofill"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearLastName"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLastName"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{personalityVM.personalityModel.txtLastName}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_last_name2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_name_onprimary_opacity_0_1_radius_10"
                        android:ellipsize="end"
                        android:fontFamily="@font/poppins"
                        android:hint="@string/lbl_fourta"
                        android:id="@+id/etLastName"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@={personalityVM.personalityModel.etLastNameValue}"
                        android:textColor="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textColorHint="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:ignore="Autofill"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearHobby"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHobby"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{personalityVM.personalityModel.txtHobby}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_hobby"/>
                    <EditText
                        android:background="@drawable/rectangle_border_name_onprimary_opacity_0_1_radius_10"
                        android:ellipsize="end"
                        android:fontFamily="@font/poppins"
                        android:hint="@string/lbl_sleep"
                        android:id="@+id/etHobby"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@={personalityVM.personalityModel.etHobbyValue}"
                        android:textColor="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textColorHint="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:ignore="Autofill"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearDateOfBirth"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDateOfBirth"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{personalityVM.personalityModel.txtDateOfBirth}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_date_of_birth"/>
                    <EditText
                        android:background="@drawable/rectangle_border_name_onprimary_opacity_0_1_radius_10"
                        android:ellipsize="end"
                        android:fontFamily="@font/poppins"
                        android:hint="@string/lbl_28_may_2002"
                        android:id="@+id/etDateOfBirth"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@={personalityVM.personalityModel.etDateOfBirthValue}"
                        android:textColor="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textColorHint="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:ignore="Autofill"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearCountry"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCountry"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{personalityVM.personalityModel.txtCountry}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_country"/>
                    <EditText
                        android:background="@drawable/rectangle_border_name_onprimary_opacity_0_1_radius_10"
                        android:ellipsize="end"
                        android:fontFamily="@font/poppins"
                        android:hint="@string/lbl_indonesian"
                        android:id="@+id/etCountry"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@={personalityVM.personalityModel.etCountryValue}"
                        android:textColor="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textColorHint="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:ignore="Autofill"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_primary_radius_14"
                    android:ellipsize="end"
                    android:fontFamily="@font/poppins"
                    android:gravity="center"
                    android:id="@+id/btnChangeSave"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_change_save"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_change_save"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
